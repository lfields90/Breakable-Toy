<div class="large-12">
  <h1>New State</h1>
  <div class="panel">
    <%= form_for @state do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :description %>
      <%= f.text_area :description %>

      <%= f.label :website_url %>
      <%= f.url_field :website_url %>

      <%= f.submit "Add new state" %>
    <%- end -%>
  </div>
</div>
