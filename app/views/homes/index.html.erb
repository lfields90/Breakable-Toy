<% if current_user %>
  <div class="page-header">
    <h1>Here is some info about you...</h1>
  </div>
  <div class="panel panel-default">
    <div class="panel-body">
      <ul>
        <li><strong>Name:</strong> <%= current_user.first_name %></li>
      </ul>
    </div>
  </div>
  <p><%= link_to("Hit the State!", states_path) %></p>
  <p><%= link_to("Hit the City!", cities_path) %></p>
  <p><%= link_to("Hit the Neighborhood!", neighborhoods_path) %></p>
<% else %>
  <%= render 'welcome' %>
<% end %>
